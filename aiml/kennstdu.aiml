<category>
	<pattern>KENNST DU *</pattern>
	<template><sraix>was ist <star/></sraix></template>
</category>

<category>
	<pattern>KENNST DU <set>name</set></pattern>
	<template>
		<condition><name>PERSON<star/></name>
			<li value="unknown">
				<random>
					<li>Ich kenne <star/> nicht pers√∂nlich.</li>
					<li>Ich habe <star/> noch nicht getroffen.</li>
					<li>Nein.</li>
				</random> <random>
				<li>Woher kennst du <star/>?</li>
			</random><think><set name="dieperson"><star/></set></think>
			</li>
			<li>Logo, <star/> ist <person><get><name>PERSON<star/></name></get></person></li>
		</condition>

	</template>
</category>
